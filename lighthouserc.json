{
    "ci": {
      "collect": {
        "url": [
          "http://localhost/",
          "http://localhost/blog/",
          "http://localhost/blog/filter-posts-by-tag/"
        ],
        "numberOfRuns": 3,
        "staticDistDir": "./dist"
      },
      "assert": {
        "preset": "lighthouse:no-pwa",
        "assertions": {
          "uses-long-cache-ttl": "off",
          "uses-http2": "off",
          "canonical": "off"
        }
      },
      "upload": {
        "target": "temporary-public-storage"
      }
    }
  }